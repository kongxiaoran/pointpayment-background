<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.impl.TokenDaoImpl">
    <insert id="insert" parameterType="com.entity.Token">
        insert into token(token,openid,session_key) values (#{token},#{openid},#{session_key})
    </insert>

    <select id="selectstatus" parameterType="String">
        select status from token where token=#{token}
    </select>

    <select id="selectall" parameterType="String">
        select from token where token=#{token}
    </select>

</mapper>